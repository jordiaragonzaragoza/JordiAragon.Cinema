		<RepositoryUrl>https://github.com/jordiaragonzaragoza/JordiAragon.Cinemas</RepositoryUrl>
		<Authors>Jordi Aragon Zaragoza</Authors>
		<UseProjectReference>false</UseProjectReference>
	</PropertyGroup>
	<ItemGroup>
		<Compile Include="..\..\GlobalSuppressions.cs">
			<Link>GlobalSuppressions.cs</Link>
		</Compile>
	</ItemGroup>
	<ItemGroup>
		<PackageVersion Include="JordiAragon.SharedKernel" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Application" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Application.Contracts" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Application.Contracts.IntegrationMessages" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Contracts" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Domain" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Domain.Contracts" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Infrastructure" Version="2.1.2" />
        <PackageVersion Include="JordiAragon.SharedKernel.Infrastructure.EntityFramework" Version="2.1.2" />
